<header class="header">
    <div class="header_logo">Node.js News</div>
    <div class="header_search">Search</div>
</header>
<div class="wrapper">
    <div class="news">
        <% if (articles != null) { %>  
            <% articles.forEach((article, index) => { %>
                <a href="/article/<%= article.source.id %> " class="news_card">
                    <img src="<%= article.urlToImage %> " alt="">
                    <h2><%= article.title %></h2>
                    <div class="news_para">
                        <p><%=article.source.name %> </p>
                        <p><%= article.author %> </p>
                    </div>
                    <p style="color: teal;"><%= article.publishedAt %> </p>
                </a>
                
            <% }) %> 
        <% } else { %>
            <h1>Error Ocurred</h1>
         
        <% } %>
    </div>
</div>
